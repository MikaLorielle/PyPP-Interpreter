def greet(name) { print("Hello, " + name); }

if True { greet("Alice"); greet("Bob"); }

class Person {
    def __init__(self, name, age) {
        self.name = name;
        self.age = age;
    }

    def introduce(self) {
        print("My name is " + self.name + " and I am " + str(self.age) + " years old.");
    }
}

p = Person("Charlie", 25);
p.introduce();
